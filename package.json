{
  "name": "autonomous-agent",
  "version": "0.1.0",
  "description": "An OpenClaw-inspired autonomous AI agent with multi-channel messaging, tool execution, heartbeat system, and local-first memory.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "onboard": "tsx src/onboard.ts",
    "setup": "node -e \"const {execSync}=require('child_process');const isWin=process.platform==='win32';execSync(isWin?'powershell -ExecutionPolicy Bypass -File setup.ps1':'bash setup.sh',{stdio:'inherit'});\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "telegraf": "^4.16.0",
    "discord.js": "^14.16.0",
    "express": "^4.21.0",
    "ws": "^8.18.0",
    "node-cron": "^3.0.3",
    "chokidar": "^4.0.0",
    "glob": "^11.0.0",
    "yaml": "^2.6.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.0",
    "uuid": "^10.0.0",
    "puppeteer": "^23.0.0",
    "imapflow": "^1.0.0",
    "nodemailer": "^6.9.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/express": "^5.0.0",
    "@types/ws": "^8.5.0",
    "@types/node-cron": "^3.0.0",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
